#include<iostream>
using namespace std;
int main(){
    int a[20],n,i,sum=0;
    cout<<"How many data do you want to insert?"<<endl;
    cin>>n;
    for(i=1;i<=n;i++){
        cout<<"Numner["<<i<<"]:";
        cin>>a[i];
        }
    for(i=1;i<=n;i=i+2){
        sum=sum+a[i];
    }
    cout<<"The sum of odd numbers is: "<<sum<<endl;
    return 0;
}
