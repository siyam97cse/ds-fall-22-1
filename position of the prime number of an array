#include<iostream>
using namespace std;
int main(){
int a[20],n,i,j,prime;
cout<<"Enter the total number of index of an Array: "<<endl;
cin>>n;
for(i=1;i<=n;i++){
    cout<<"Number["<<i<<"]:"<<endl;
    cin>>a[i];
}
for(i=1;i<=n;i++){
    for(j=2;j<=a[i];j++){
        if(a[i]%j==0){
            break;
        }
    }
    if(a[i]==j){
        cout<<a[i]<<" pos:"<<i<<endl;
    }
 }

return 0;
}
